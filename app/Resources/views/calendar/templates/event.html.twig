<div class="modal fade" aria-hidden="true" aria-labelledby="editNewEvent" role="dialog" tabindex="-1" data-show="false">
	<div class="modal-dialog">
		<form class="modal-content" action="#" method="post" role="form">
			<div class="modal-header">
				<button type="button" class="close" aria-hidden="true" data-dismiss="modal">Ã—</button>
				<h4 class="modal-title">{% trans %}Event details{% endtrans %}</h4>
			</div>
			<div class="modal-body">
				<div class="col-xs-12 col-lg-12">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Customer name{% endtrans %}:</label>
						<a href="<%- userUrl %>" class="form-control"><%- userName %></a>
					</div>
				</div>
				
				<% if (type === 'appointmentDate' || type === 'appointmentTwoDate' || type === 'appointmentThreeDate' || type === 'appointmentFourDate' || type === 'appointmentFiveDate' || type === 'appointmentSixDate' ) { %>
				<div class="col-xs-12 col-lg-12">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Starts{% endtrans %}:</label>
						<div class="form-control"><%- start.format('L') %></div>
					</div>
				</div>
				<!--div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Starts time{% endtrans %}:</label>
						<div class="form-control"><%- start.format('LT') %></div>
					</div>
				</div-->
				<div class="col-xs-12 col-lg-12">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Clinic{% endtrans %}:</label>
						<div class="form-control"><%- clinicName %></div>
					</div>
				</div>
				<% } else { %>
				<div class="col-xs-12 col-lg-12">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Starts{% endtrans %}:</label>
						<div class="form-control"><%- start.format('L') %></div>
					</div>
				</div>
				<div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Accommodation{% endtrans %}:</label>
						<div class="form-control"><%- accommodation %></div>
					</div>
				</div>
				<div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Accommodation address{% endtrans %}:</label>
						<div class="form-control"><%- accommodationAddress %></div>
					</div>
				</div>
				<div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Night load to client{% endtrans %}:</label>
						<div class="form-control"><%- nightLoadToClient %></div>
					</div>
				</div>
				<div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Night load to HC{% endtrans %}:</label>
						<div class="form-control"><%- nightLoadToHC %></div>
					</div>
				</div>
				<div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Transport load to client{% endtrans %}:</label>
						<div class="form-control"><%- transportLoadToClient %></div>
					</div>
				</div>
				<div class="col-xs-12 col-lg-6">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Transport load to HC{% endtrans %}:</label>
						<div class="form-control"><%- transportLoadToHC %></div>
					</div>
				</div>
				<% } %>

				<% if (notes) { %>
				<div class="col-xs-12">
					<div class="form-group form-material">
						<label class="form-control-label">{% trans %}Notes{% endtrans %}:</label>
						<div class="form-control"><%- notes %></div>
					</div>
				</div>
				<% } %>
			</div>
			<div class="modal-footer">
				<div class="form-actions">
					<button type="button" class="btn btn-default btn-pure waves-effect" data-dismiss="modal">{% trans %}Close{% endtrans %}</button>
				</div>
			</div>
		</form>
	</div>
</div>
